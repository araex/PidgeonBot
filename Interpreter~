#!/bin/bash

# this files gets generated during runtime

irc_msg_interpret(){
    case "$3" in

        ":!ddate" )
            . modules/ddate
            pidgeonbot_module_ddate ${1} ${2} ${3}
            ;;

        ":!echo"|":!2ndecho" )
            . modules/echo
            pidgeonbot_module_echo ${1} ${2} ${3}
            ;;

        *)
            ;;
    esac
}
 modules/echo
            pidgeonbot_module_echo ${1} ${2} ${3}
            ;;

        ":!echo"|":!2ndecho" )
            . modules/echo~
            pidgeonbot_module_echo~ ${1} ${2} ${3}
            ;;

        *)
            ;;
    esac
}
